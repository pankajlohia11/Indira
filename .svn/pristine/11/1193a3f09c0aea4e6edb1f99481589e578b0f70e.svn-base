    <script type="text/javascript">

        function getdatepic() {
            $datepickerEl = $('.datepicker');
            if ($datepickerEl.length > 0) {
                $datepickerEl.each(function () {
                    var element = $(this);
                    var format = element.data('format')
                    element.datetimepicker({
                        format: format
                    });
                });
            }
        }
        /*function deleteRecord(e, f) {
            if ($(f).attr("id").toString().indexOf("DeleteYes") > -1) {
                alertmessage(e, $(f).attr("id").toString());
            }
            else {
                message = "You don't access to do this operation, please contact admin.";
                toastr["error"](message, "Notification");
            }
        }*/

        function deleteRecordConfirm(e, f,deleteOrRestore,ctrlName,ctrlAction) {
            //debugger;
            var res = false;
            var deleteUrl = '/' + ctrlName +  '/' + ctrlAction;
            //alert(e + f + ctrlName + ctrlAction);
            //var res = false;
            //if (b)
            //    res = $scope.Isdelete;
            //else
            //    res = $scope.Isrestore
            //res = $scope.Isdelete;
            var rowId = f;
            res = true;
            if (res) {
                var cName = ctrlName;
                var cAction = ctrlAction;
                $.ajax({
                    type: "GET",
                    url: deleteUrl,
                    data: {
                        id: rowId,
                        type: deleteOrRestore

                    },
                    contentType: "application/json;charset=utf-8",
                    dataType: "json",
                    success: function (result) {
                        if (result.indexOf("ERR") > -1) {
                            $("#spanErrMessage1").html(result);
                            $("#spanErrMessage2").html(result);
                            if ($("#exceptionmessage").length)
                                $("#exceptionmessage").trigger("click");
                        }
                        else {
                            if (result == "Success") {
                                message = 'Record Deleted Successfully. You can retrive it from Restore page';
                                toastr["success"](message, "Notification");
                                var ctrl = "#" + f;
                                $(ctrl).parent().parent().fadeOut(800, function () { $(this).remove(); })
                            }
                            else {
                                message = 'Failed To Delete';
                                toastr["error"](message, "Notification");
                            }
                        }
                    },
                    error: function (response) {
                        alert(response);
                        if ($("#exceptionmessage").length)
                            $("#exceptionmessage").trigger("click");
                    }
                });
            }
        }

        function HideBorder(e) {
            $(e).css("border", "1px solid #efefef");
        }
        function alertmessage(a, f)
        {
            toastr['warning']("Are you sure you want to delete this record?<br><button class='btn btn-greensea btn-sm mb-10' style='padding:5px;' type='button' onclick='deleteRecordConfirm(\"" + a + "\",\"" + f + "\");' class='btn dark'>YES</button> &nbsp <button type='button' style='padding:5px;' class='btn btn-red btn-sm mb-10'>NO</button>", "Alert");
        }
        function alertmessageModified(evt,record,deleteOrRestore,ctrlName, ctrlAction)
        {
            toastr['warning']("Are you sure you want to delete this record?<br><button class='btn btn-greensea btn-sm mb-10' style='padding:5px;' type='button' onclick='deleteRecordConfirm(\"" + evt + "\",\"" + record + "\",\"" + deleteOrRestore + "\",\"" + ctrlName + "\",\"" + ctrlAction + "\");' class='btn dark'>YES</button> &nbsp <button type='button' style='padding:5px;' class='btn btn-red btn-sm mb-10'>NO</button>", "Alert");
        }
        function alertfollowupmessage(a) {
            toastr.options = {
                "closeButton": true,
                "debug": false,
                "positionClass": "toast-top-center",
                "onclick": null,
                "showDuration": "1000",
                "hideDuration": "1000",
                "timeOut": "50000",
                "extendedTimeOut": "1000",
                "showEasing": "swing",
                "hideEasing": "linear",
                "showMethod": "show",
                "hideMethod": "fadeOut"
            };
            toastr['error']("Are you sure you want to delete this record?<br><button style='height:30px;text-align:center;font-size:small' type='button' onclick='closefollowupbutton(\"" + a + "\");' class='btn dark'>YES</button><button type='button' style='height:30px;text-align:center;font-size:small' class='btn default pull-right'>NO</button>", "Alert");
        }

    </script>
