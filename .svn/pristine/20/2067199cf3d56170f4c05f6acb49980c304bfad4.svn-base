    function InsertRecords() {

        var YarnID = $("#PRODUCT_YARN_ID").val();
        if (YarnID != 0) {
            YarnID = YarnID;
        }
        else {
            YarnID = "0";
        }

        var YarnCode = $("#PRODUCT_YARN_CODE").val();
        if (YarnCode != 0) {
            YarnCode = YarnCode;
        }
        else {
            YarnCode = "0";
        } 

        var txtShortName = $("#txtShortName").val();
        var drpUOM = $("#drpUOM").val();
        var drpCount = $("#drpCount").val();
        var txtCountPercentage = $("#txtCountPercentage").val();
        var drpBlend1 = $("#drpBlend1").val();
        var txtBlend1 = $("#txtBlend1").val();
        var drpBlend2 = $("#drpBlend2").val();
        var txtBlend2 = $("#txtBlend2").val();
        var drpBlend3 = $("#drpBlend3").val();
        var txtBlend3 = $("#txtBlend3").val();
        var drpBlend4 = $("#drpBlend4").val();
        var txtBlend4 = $("#txtBlend4").val();

        var drpSubRange1 = $("#drpSub-Range1").val();
        var drpSubRange2 = $("#drpSub-Range2").val();
        var drpSubRange3 = $("#drpSub-Range3").val();
        var drpSubRange4 = $("#drpSub-Range4").val();
        var drpSubRange5 = $("#drpSub-Range5").val();
        var drpPackage = $("#drpPackage").val();
        var drpCottontype = $("#drpCottontype").val();

        $.ajax({
            type: "GET",
            url: '@Url.Action("ET_Admin_Store_Add", "ET_Admin_Store")',
            contentType: "application/json;charset=utf-8",
            dataType: "json",
            data: {
                YarnID: YarnID,
                YarnCode: YarnCode,
                UOM: drpUOM,
                ShortName: txtShortName,
                drpCount: drpCount,
                tCountPercentage: txtCountPercentage,
                drpBlend1: drpBlend1,
                txtBlend1: txtBlend1,
                drpBlend2: drpBlend2,
                txtBlend2: drpBlend2,
                drpBlend3: drpBlend3,
                txtBlend3: txtBlend3,
                drpBlend4: drpBlend4,
                txtBlend4: txtBlend4,
                SubRange1: drpSubRange1,
                SubRange2: drpSubRange2,
                SubRange3: drpSubRange3,
                SubRange4: drpSubRange4,
                SubRange5: drpSubRange5,
                Package: drpPackage,
                Cottontype: drpCottontype
            },
            success: function (result) {
                if (result.indexOf("ERR") > -1) {
                    $("#spanErrMessage1").html(result);
                    $("#spanErrMessage2").html(result);
                    if ($("#exceptionmessage").length)
                        $("#exceptionmessage").trigger("click");
                }
                else {
                    if (result == "Success") {
                        createRecords();
                        if (StoreID == 0) {
                            message = 'Record Inserted Successfully.';
                            toastr["success"](message, "Notification");
                        }
                        else {
                            message = 'Record Updated Successfully.';
                            toastr["success"](message, "Notification");
                        }
                    }
                    else if (result == "Failed") {
                        message = 'Failed to do this operation.';
                        toastr["error"](message, "Notification");
                    }
                    else if (result.indexOf("Validation") > -1) {
                        toastr["error"](result, "Notification");
                    }
                }
            },
            error: function (response) {
               // alert(response);
                message = "You don't access to do this operation, please contact admin.";
                toastr["error"](message, "Notification");
            }
        });
    };

    function Validation() {
        debugger;
        var drpBlend1 = $("#drpBlend1").val();
        var txtBlend1 = parseInt($("#txtBlend1").val());
        var drpBlend2 = $("#drpBlend2").val();
        var txtBlend2 = parseInt($("#txtBlend2").val());
        var drpBlend3 = $("#drpBlend3").val();
        var txtBlend3 = parseInt($("#txtBlend3").val());
        var drpBlend4 = $("#drpBlend4").val();
        var txtBlend4 = parseInt($("#txtBlend4").val());
        var total = txtBlend1 + txtBlend2 + txtBlend3 + txtBlend4;
        if (total != 100)
        {
            message = "It Should not be less than or greater than 100%";
            toastr["error"](message, "Notification");
        }
        else if (drpBlend1 == drpBlend2 || drpBlend3 == drpBlend4) {
            message = " Blend 1, Blend 2, Blend 3, Blend 4 should be different";
            toastr["error"](message, "Notification");
        }
        else if (drpBlend1 == drpBlend3 || drpBlend2 == drpBlend4) {
            message = " Blend 1, Blend 2, Blend 3, Blend 4 should be different";
            toastr["error"](message, "Notification");
        }
        else if (drpBlend1 == drpBlend4 || drpBlend2 == drpBlend3) {
            message = " Blend 1, Blend 2, Blend 3, Blend 4 should be different";
            toastr["error"](message, "Notification");
        }
        else {
            InsertRecords();
        }
    };

</script>
