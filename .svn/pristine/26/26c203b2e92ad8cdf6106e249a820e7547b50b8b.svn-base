    <section id="content">
        <div class="page page-shop-single-order" id="mainDiv" ng-controller="AC_ShipmentCtrl">
            <div class="pageheader">

                <h2>{{Type}} Shipment<span>@*You can place subtitle here*@</span></h2>

                <div class="page-bar">

                    <ul class="page-breadcrumb">
                        <li>
                            <a href="../Home/Index"><i class="fa fa-home"></i> Indra</a>
                        </li>
                        <li>
                            <a href="#">Admin</a>
                        </li>
                        <li>
                            <a href="#">{{Type}} Shipment</a>
                            <a style="display:none;" id="PAthid" target="_blank">click to download</a>
                        </li>
                    </ul>

                </div>

            </div>

            <div class="row" id="mycontroller">
                <div class="col-md-12">
                    <section class="tile" id="div_View">
                        <!--[div_view] All view list contents datas will be written here-->
                        <!-- tile header -->
                        <div class="tile-header dvd-btm">
                            <h1 class="custom-font"><strong>{{Type}} Shipment</strong> List</h1>
                            <ul class="controls">
                                <li style="padding:5px">
                                    <!-- function createRecords() is used for add records. Once click this button it will navigate to Create page.-->
                                    <a tabindex="0" ng-click="createnew()">
                                        <span class="btn btn-slategray btn-xs mb-10">
                                            Create
                                        </span>
                                    </a>
                                </li>
                                <!-- function restoreRecords() is used for displaying Deleted records. Once click on this button it will navigate to restore page.-->
                                <li style="padding:5px"><a tabindex="0" ng-click="restoreRecords();"><span class="btn btn-drank btn-xs mb-10"><i class="fa fa-undo"></i> Restore</span></a></li>
                            </ul>
                        </div>
                        <!-- /tile header -->
                        <!-- tile body -->
                        <div class="tile-body">
                            <div class="row">
                                <div class="col-md-6"><div id="tableTools"></div></div>
                                <div class="col-md-6"><div id="colVis"></div></div>
                            </div>
                            <!-- Table created for displaying View content-->
                            <table class="table table-custom contattbl" id="advancedusage">
                                <thead>
                                    <tr>
                                        <th>ETD</th>
                                        <th>ETA</th>
                                        <th>Code</th>
                                        <th>Customer</th>
                                        <th>Supplier</th>

                                        <th ng-if="S_Type==1">Invoice No</th>
                                        <th>Invoice Amount</th>
                                        <td>Action</td>
                                    </tr>

                                </thead>
                                <tbody>
                                    <tr ng-repeat="shipment in shipmentlist">
                                        <td>{{shipment.S_ETD.replace('/Date(','').replace(')/','') | date:'yyyy-MM-dd'}}</td>
                                        <td>{{shipment.S_ETA.replace('/Date(','').replace(')/','') | date:'yyyy-MM-dd'}}</td>
                                        <td>{{shipment.S_Code}}</td>
                                        <td>{{shipment.COM_DISPLAYNAME}}</td>
                                        <td>{{shipment.SuppName}}</td>

                                        <td ng-if="S_Type==1">{{shipment.S_INV_NO}}</td>
                                        <td style="text-align:right;">{{(shipment.S_INV_AMT).toLocaleString("es-ES", { minimumFractionDigits: 2 })}} €</td>
                                        <td>
                                            <a ng-click="editRecords(shipment.S_ID);" ng-if="shipment.S_STATUS != 1" value="{{ shipment.S_ID }}" style="color:forestgreen;padding:5px;" title="Edit"><i class="fa fa-edit"></i></a>
                                            <a ng-click="ViewRecords(shipment.S_ID);" value="{{ shipment.S_ID }}" style="color:darkblue;padding:5px;" title="View"><i class="fa fa-eye"></i></a>
                                            <a ng-click="Restoredeleterecords(shipment.S_ID,$event,true);" ng-if="shipment.S_STATUS!= 1" style="color:red;padding:5px;" title="Delete"><i class="fa fa-trash"></i></a>
                                            <a ng-click="generateDebitNote(shipment.S_ID,shipment.S_INV_AMT);" value="{{ shipment.S_ID }}" ng-if="shipment.S_Type=='1' && shipment.S_STATUS == 0" style="color:{{shipment.S_DebitNoteApprovalStatus == 0 ? 'darkgreen' : 'red'}};padding:5px;" title="{{shipment.S_DebitNoteApprovalStatus == 0 ? 'Generate Debit Note' : 'Debit Note Is Rejected. ReGenerate Debit Note.'}}"><i class="fa fa-file-archive-o"></i></a>
                                            <a ng-click="PrintRecords(shipment.S_ID,'E');" value="{{ shipment.S_ID }}" ng-if="shipment.S_DebitNoteApprovalStatus==1 && shipment.S_Type==1" style="color:darkgreen;padding:5px;" title="Print DebitNote"><i class="fa fa-print"></i></a>
                                            <a ng-click="PrintRecordsForTrading(shipment.S_ID,'E');" value="{{ shipment.S_ID }}" ng-if="shipment.S_Type==2" style="color:darkgreen;padding:5px;" title="Print DebitNote"><i class="fa fa-print"></i></a>
                                            <a ng-click="ShipmenttoInvoice(shipment.S_ID);" value="{{ shipment.S_ID }}" ng-if="shipment.S_Type==2" style="color:darkgreen;padding:5px;" title="Link to invoice"><i class="fa fa-money"></i></a>
                                            <a ng-click="MailSentForOrder(shipment.S_ID);" value="{{ shipment.S_ID }}" ng-if="shipment.S_Type==2" style="color:darkgreen;padding:5px;" title="Link to invoice"><i class="fa fa-envelope"></i></a>
                                            <a ng-click="CommissionRecieve(shipment.S_ID);" value="{{ shipment.S_ID }}" ng-if="shipment.S_DebitNoteApprovalStatus==1 && shipment.S_CommissionRecievedStatus== false && shipment.S_Type==1" style="color:darkgreen;padding:5px;" title="Commission Recieved"><i class="fa fa-money"></i></a>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                        <!-- /tile body -->
                    </section>
