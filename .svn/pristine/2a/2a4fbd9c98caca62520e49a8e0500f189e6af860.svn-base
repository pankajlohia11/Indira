                    <h3>Customer : <strong class="text-greensea">@ViewBag.CustomerName</strong></h3>
                    <span class="col-sm-2 controls pull-right">
                        @Html.DropDownList("drpCustomers", new SelectList(Model.CustomerMasterList, "COM_ID", "COM_NAME"), "Choose Company", new { @class = "form-control chosen-select", onchange = "ChangeCustomer(this);", @id = "drpCustomers" })
