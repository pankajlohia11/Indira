                                                                    {
                                                                        BusinessEntity.EntityModels.EntityClasses dbcontext = new BusinessEntity.EntityModels.EntityClasses();
                                                                        int con = Convert.ToInt32(item.COM_COUNTRY);
                                                                        country = dbcontext.locations.Where(m => m.location_id == con).ToList()[0].name;
                                                                        int sta = Convert.ToInt32(item.COM_STATE);
                                                                        state = dbcontext.locations.Where(m => m.location_id == sta).ToList()[0].name;
                                                                        int cit = Convert.ToInt32(item.COM_CITY);
                                                                        city = dbcontext.locations.Where(m => m.location_id == cit).ToList()[0].name;
                                                                    }
                                                                    catch { }
                                                                }
                                                                <li>@city</li>
                                                                <li>@state</li>
                                                                <li>@country</li>
                                                                <li>@item.COM_ZIP</li>
                                                            </ul>
                                                        </div>
