                                                                <li>@item.COM_MOBILE</li>
                                                            </ul>
                                                        </div>
                                                        <!-- /col -->

                                                    </div>
                                                    <!-- /col -->
                                                    <!-- col -->
                                                    <div class="col-md-3">
                                                        <p class="text-uppercase text-strong mb-10 custom-font">Customer Type</p>
