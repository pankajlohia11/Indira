<script type="text/javascript">
    function DiscountValidation(s)
    {
        debugger;
            try {
                var re = /^[\d.]+(\,\d*)?$/;
                var txt = s.value;
                if (txt != "") {
                    if (!re.test(txt)) {
                        toastr["error"]("Invalid Data", "Notification");
                        s.value = '';
                        return false;
                    }
                    else {
                        var td = $(s).parent().parent().find("td");
                        if ($(td[3]).find("input").val() != "") {
                            var price = parseFloat($(td[3]).find("input").val().split('.').join("").replace(",", "."));
                            var DiscountPer = s.value;
                            var DiscountAmt = 0;
                            DiscountAmt = (price * DiscountPer / 100);
                            DiscountAmt = price - DiscountAmt;
                            //$(td[3]).find("input").val(DiscountAmt.toLocaleString("es-ES", { minimumFractionDigits: 2 }));
                            

                        }
                        else {
                            toastr["error"]("Enter the Price", "Notification");
                            $(s).val("");
                        }
                    }
                }
                
            }
            catch (e) {
                toastr["error"]("Invalid Data", "Notification");
                $(s).val("");
            }
    }
