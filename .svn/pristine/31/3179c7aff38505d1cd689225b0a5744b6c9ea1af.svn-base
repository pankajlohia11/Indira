                                //                   where state.location_id == Countryid
                                //                   select state.name).FirstOrDefault();

                                //var CityName = (from state in dbc.locations
                                //                where state.location_id == Cityid
                                //                select state.name).FirstOrDefault();

                                <tr>
                                    <td>@item.COM_ID</td>
                                    <td>@item.COM_NAME</td>
                                    <td>@item.COM_COUNTRY</td>
                                    <td>@item.COM_CITY</td>
                                    <td>@item.COM_MOBILE</td>
                                    <td>
                                        <a onclick="editRecords(@item.COM_ID,this);" value="@item.COM_ID" id="@ViewBag.Edit@i" style="color:forestgreen;padding:5px;"><i class="fa fa-edit"></i></a>

                                        <a onclick="ViewRecords(@item.COM_ID,this);" value="@item.COM_ID" id="@ViewBag.View@i" style="color:darkblue;padding:5px;"><i class="fa fa-eye"></i></a>

                                        <a onclick="deleteRecord(@item.COM_ID,this);" id="@ViewBag.Delete@i" style="color:red;padding:5px;"><i class="fa fa-trash"></i></a>
                                    </td>
                                </tr>
                                i++;
                            }
                        }
                    </table>
                </div>
                <!-- /tile body -->
            </section>
            <!--The user can have a option to create and edit records-->
            <section class="tile" id="div_Edit" style="display:none">
                <!-- tile header -->
                <div class="tile-header dvd-btm">
                    <h1 class="custom-font"><strong>Create</strong> Company Master</h1>
                    <ul class="controls">
                        <li style="padding:5px">
                            <!-- function showRecords() is used for return back to view from the current page.ie it actually hide the other pages except view -->
                            <a tabindex="0" onclick="showRecords();">
                                <span class="btn btn-hotpink btn-xs mb-10">
                                    <i class="fa fa-arrow-left"></i> Back
                                </span>
                            </a>
                        </li>
                    </ul>
                </div>
                <!-- This Below code is used for Tab purpose -->
                @*<ul class="nav nav-tabs tabs-dark" style="width:252px;margin-left:25px;background-color:#fff;">
                        <li role="presentation"><a style="background-image:url(../../Assets/images/companydetails.png);height:60px;width:84px;" onclick="companydetais();"></a><i class="icon-calculator"></i></li>
                        <li role="presentation"><a style="background-image:url(../../Assets/images/Contacts.png);height:60px;width:84px;" class="Contacts hidden" onclick="contactdetails();"></a></li>
                        <li role="presentation"><a style="background-image:url(../../Assets/images/BankAccounts.gif);height:60px;width:84px;margin-top:-4px;" onclick="bankdetails();"></a></li>
                    </ul>*@
                <!-- /tile header -->
                <!-- tile body -->
                <div id="div_company">
                    <div class="tile-body">
                        <div name="form1" role="form" id="form1" data-parsley-validate>
                            <h5><label><strong>Company Info</strong></label></h5>
                            <div class="row">
                                <div class="form-group col-md-6">
                                    <label for="Email">Company Code</label>
                                    <input type="text" name="CompanyCode" id="txtCompanyCode" class="form-control" disabled="@ViewBag.AutoManual">
                                    <input type="text" name="txtComID" id="txtComID" hidden>
                                </div>
                            </div>

                            <div class="row">

                                <div class="form-group col-md-6">
                                    <label for="Email">Company Name</label>
                                    <input type="text" name="CompanyName" id="txtCompanyName" class="form-control" data-parsley-trigger="change" pattern="/^[a-zA-Z]+$/" required>
                                    <input type="text" name="txtComID" id="txtComID" hidden>
                                </div>
                                <div class="form-group col-md-6">
                                    <label for="Url">Display Name</label>

                                    <input type="text" name="DisplayName" id="txtDisplayName" class="form-control" data-parsley-trigger="change" pattern="/^[a-zA-Z]+$/" required>
                                </div>
                            </div>

                            <div class="row">

                                <div class="form-group col-md-6">
                                    <label for="Digits">UstId</label>
                                    <input type="text" name="UstId" id="UstId" class="form-control">
                                </div>
                                <div class="form-group col-md-6">
                                    <label for="Numbers">Tax No</label>

                                    <input type="text" name="TaxNo" id="txtTaxNo" class="form-control">
                                </div>
                            </div>

                            <h5> <label><strong>Address Info</strong></label></h5>
                            <div class="row">
                                <div class="form-group col-md-6">
                                    <label for="Alphanumeric">Address</label>
                                    <input type="text" name="Address" id="txtAddress" class="form-control" data-parsley-trigger="change" data-parsley-type="alphanum" required>
                                </div>
                                <div class="form-group col-md-6">
                                    <label for="Alpha">Country</label>
                                    @*<select class="form-control"></select>*@
                                    @Html.DropDownList("Country", new SelectList(ViewBag.Country, "location_id", "name"), new { @class = "form-control chosen-select", onchange = "State()", @id = "Country" })
                                </div>
                            </div>

                            <div class="row">
                                <div class="form-group col-md-6">
                                    <label for="Alpha">State</label>
                                    <select class="form-control" id="drpState" onchange="City()"></select>
                                    @*@Html.DropDownList("City", new SelectList(ViewBag.City, "location_id", "name"), new { @class = "form-control chosen-select" })*@
                                </div>
                                <div class="form-group col-md-6">
                                    <label for="Alpha">City</label>
                                    <select class="form-control" id="drpCity"></select>
                                    @*@Html.DropDownList("State", new SelectList(ViewBag.State, "location_id", "name"), new { @class = "form-control chosen-select", onchange = "City()", @id = "City" })*@
                                </div>
                            </div>

                            <div class="row">
                                <div class="form-group col-md-6">
                                    <label for="Alphanumeric">Zip</label>
                                    <input type="text" name="Zip" id="txtZip" class="form-control" data-parsley-trigger="change" data-parsley-type="digits" required>
                                </div>
                            </div>

                            <h5><label><strong>Contact Info</strong></label></h5>
                            <div class="row">
                                <div class="form-group col-md-6">
                                    <label for="Alphanumeric">Email</label>
                                    <input type="email" name="Email" id="txtEmail" class="form-control" data-parsley-trigger="change" required>
                                </div>
                                <div class="form-group col-md-6">
                                    <label for="Alpha">Fax</label>

                                    <input type="text" name="Fax" id="txtFax" class="form-control" data-parsley-trigger="change" data-parsley-type="digits">
                                </div>
                            </div>
                            <div class="row">

                                <div class="form-group col-md-6">
                                    <label for="Alphanumeric">Phone</label>
                                    <input type="text" name="Phone" id="txtPhone" class="form-control" data-parsley-trigger="change" data-parsley-type="digits">
                                </div>
                                <div class="form-group col-md-6">
                                    <label for="Alpha">Mobile</label>

                                    <input type="text" name="Mobile" id="txtMobile" class="form-control" data-parsley-trigger="change" data-parsley-type="digits">
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label class="control-label">
                                            Company Type
                                        </label>
                                    </div>

                                    <div class="col-sm-6">
                                        <select class="form-control" id="companytype">
                                            <option name="companytype" value="0">Customer</option>
                                            <option name="companytype" value="1">Supplier</option>
                                            <option name="companytype" value="2">Both</option>
                                        </select>
                                    </div>
                                </div>

                                <div class="form-group col-md-6">
                                    <label for="Alpha">Remarks</label>
                                    <input type="text" name="Remarks" id="txtRemarks" class="form-control" data-parsley-trigger="change" data-parsley-type="alphanum">
                                </div>
                            </div>


                            <section class="tile">
                                <!-- tile header -->
                                <div style="background-color:cadetblue;color:white;" class="tile-header dvd dvd-btm collapsed" data-toggle="collapse" href="#collapseExample" aria-expanded="false" aria-controls="collapseExample">
                                    <h1 class="custom-font"><strong>Contact</strong> Details</h1>
                                </div>
                                <!-- /tile header -->
                                <!-- tile body -->
                                <div class="tile-body">

                                    <div class="collapse" id="collapseExample">
                                        <div class="row">
                                            <div class="table-responsive">
                                                <table class="table table-custom" id="editable-usage">
                                                    <thead>
                                                        <tr>
                                                            <th>Title</th>
                                                            <th>First Name</th>
                                                            <th>Last Name</th>
                                                            <th>Job Title</th>
                                                            <th>Phone</th>
                                                            <th>Fax</th>
                                                            <th>Mobile</th>
                                                            <th>Email</th>
                                                            <th>Remarks</th>
                                                            <th style="width: 160px;" class="no-sort">Actions</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody id="contactbody"></tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                                <!-- /tile body -->

                            </section>

                            <div class="tile-footer text-center bg-tr-black lter dvd dvd-top text-center  m-t-20">
                                <!-- SUBMIT BUTTON -->
                                <!-- Button used for submitting the record -->
                                <button type="submit" class="btn btn-success" id="formSubmit" onclick="validate();">Submit</button>
                            </div>
                        </div>

                    </div>
                </div>
            </section>

            <section class="tile" id="div_Restore" style="display:none">
                <div id="restorediv">
                </div>
            </section>

        </div>
    </div>
</section>