    function restoreRecords(f) {
        if ($(f).attr("id").toString().indexOf("RestoreYes") > -1) {
            $.ajax({
                type: "GET",
                url: '@Url.Action("ET_Master_Company_Restore", "ET_Admin_Company")',
                contentType: "application/json; charset=utf-8",
