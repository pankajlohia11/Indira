            var CompanyName = $("#txtCompanyName").val();
            var DisplayName = $("#txtDisplayName").val();
            var UstId = $("#UstId").val();
            var TaxNo = $("#txtTaxNo").val();
            var Address = $("#txtAddress").val();
            var Country = $("#Country").val();
            var State = $("#drpState").val();
