            $scope.COM_NAME = "";
            $scope.COM_DISPLAYNAME = "";
            $scope.COM_USTID = "";
            $scope.Tax_No = "";
            $scope.COM_STREET = "";
            $scope.COM_ZIP = "";
            $scope.COM_EMAIL = "";
            $scope.COM_FAX = "";
