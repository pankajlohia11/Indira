            $scope.OrderList = OrderLi;
            $scope.OneToOneList = OneToOneLi;
            $scope.OneToManyList = OneToManyLi;
            $scope.PaymentList = PaymentLi;
            $scope.ScheduleList = ScheduleLi;
            $scope.ShipmentList = ShipmentLi;
            $scope.GetCompanyCommunication();
