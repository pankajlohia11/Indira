                    <!-- tile body -->
                    <div id="div_company">
                        <div class="tile-body">
                            <div name="form1" role="form" id="form1" data-parsley-validate>
                                <h5><label><strong>Company Info</strong></label></h5>
                                <div class="row">
                                    <div class="form-group col-md-6">
                                        <label for="Email">Company Code *</label>
                                        <input type="text" name="CompanyCode" id="txtCompanyCode" maxlength="50" ng-model="COM_CODE" class="form-control" disabled="@ViewBag.AutoManual" @ViewBag.Required>
                                        <input type="text" name="txtComID" id="txtComID" ng-model="COM_ID" class="hidden">
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="form-group col-md-6">
                                        <label for="Email">Company Name *</label>
                                        <input type="text" name="CompanyName" id="txtCompanyName" ng-model="COM_NAME" class="form-control" data-parsley-trigger="change" required maxlength="150">
                                    </div>

                                    <div class="form-group col-sm-6">
                                        <label class="control-label"> Company Type</label>
                                        <select data-ng-model="selectedcompanytype" ng-change="CompanyTypeChange()" class="form-control" id="companytype">
                                            <option name="companytype" value="0">Customer</option>
                                            <option name="companytype" value="1">Supplier</option>
                                            <option name="companytype" value="2">Both</option>
                                        </select>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="form-group col-md-6">
                                        <label for="Url">Display Name *</label>
                                        <input type="text" name="DisplayName" id="txtDisplayName" ng-model="COM_DISPLAYNAME" class="form-control" data-parsley-trigger="change" required maxlength="100">
                                    </div>

                                </div>

                                <div class="row">

                                    <div class="form-group col-md-6">
                                        <label for="Digits">UstId *</label>
                                        <input type="text" name="UstId" id="UstId *" ng-model="COM_USTID" data-parsley-trigger="change" maxlength="50" class="form-control" required>
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label for="Numbers">Tax No*</label>

                                        <input type="text" name="TaxNo" id="txtTaxNo" ng-model="Tax_No" class="form-control" data-parsley-trigger="change" requied maxlength="50">
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="form-group col-md-6">
                                        <label for="Digits">Credit Days *</label>
                                        <input type="text" name="txtcreditdays" id="txtcreditdays" maxlength="5" data-parsley-trigger="change" data-parsley-type="digits" required ng-model="COM_CreditDays" class="form-control">
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label for="Url">Sales Person</label>
                                        <select data-ng-model="COM_Sales_Person" id="drpSalesPerson" class="form-control chosen-select" ng-change="SalesPersonChange()">
                                            <option value="0">-Select-</option>
                                            <option ng-repeat="salesperson in salespersons" value="{{salesperson.USER_ID}}">
                                                {{salesperson.DISPLAY_NAME}}
                                            </option>
                                        </select>
                                    </div>
                                </div>
                                <h5> <label><strong>Address Info *</strong></label></h5>
                                <div class="row">
                                    <div class="form-group col-md-6">
                                        <label for="Alphanumeric">Address *</label>
                                        <input type="text" name="Address" id="txtAddress" ng-model="COM_STREET" class="form-control" data-parsley-trigger="change" required>
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label for="Alpha">Country *</label>
                                        <select data-ng-model="selectedcountry" id="drpCountry" ng-change="CountryChange()" data-parsley-trigger="change" class="form-control chosen-select" required>
                                            <option value="">-Select-</option>
                                            <option ng-repeat="country in countrys" value="{{country.location_id}}">
                                                {{country.name}}
                                            </option>
                                        </select>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="form-group col-md-6">
                                        <label for="txtState">State *</label>
                                        <input type="text" name="txtState" id="txtState" ng-model="COM_STATE" class="form-control" data-parsley-trigger="change" required>
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label for="txtCity">City *</label>
                                        <input type="text" name="txtCity" id="txtCity" ng-model="COM_CITY" class="form-control" data-parsley-trigger="change" required>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="form-group col-md-6">
                                        <label for="Alphanumeric">Zip *</label>
                                        <input type="text" name="Zip" id="txtZip" class="form-control" ng-model="COM_ZIP" data-parsley-trigger="change" data-parsley-type="digits" maxlength="20" required>
                                    </div>
                                </div>

                                <h5><label><strong>Contact Info</strong></label></h5>
                                <div class="row">
                                    <div class="form-group col-md-6">
                                        <label for="Alphanumeric">Email *</label>
                                        <input type="email" name="Email" id="txtEmail" class="form-control" ng-model="COM_EMAIL" data-parsley-trigger="change" required maxlength="50">
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label for="Alpha">Fax</label>

                                        <input type="text" name="Fax" id="txtFax" class="form-control" ng-model="COM_FAX" data-parsley-trigger="change" data-parsley-type="digits" maxlength="20">
                                    </div>
                                </div>
                                <div class="row">

                                    <div class="form-group col-md-6">
                                        <label for="Alphanumeric">Phone</label>
                                        <input type="text" name="Phone" id="txtPhone" class="form-control" ng-model="COM_PHONE" data-parsley-trigger="change" pattern="^[\d\+\-\.\(\)\/\s]*$" maxlength="20">
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label for="Alpha">Mobile</label>

                                        <input type="text" name="Mobile" id="txtMobile" class="form-control" ng-model="COM_MOBILE" data-parsley-trigger="change" pattern="^[\d\+\-\.\(\)\/\s]*$" maxlength="20">
                                    </div>
                                </div>
                                <div class="row">


                                    <div class="form-group col-md-6">
                                        <label for="Alpha">Remarks </label>
                                        <input type="text" name="Remarks" id="txtRemarks" class="form-control" ng-model="COM_REMARKS" data-parsley-trigger="change" maxlength="500">
                                    </div>
                                </div>


                                <section class="tile">
                                    <!-- tile header -->
                                    <div style="background-color:cadetblue;color:white;" class="tile-header dvd dvd-btm collapsed" data-toggle="collapse" href="#collapseExample" aria-expanded="false" aria-controls="collapseExample">
