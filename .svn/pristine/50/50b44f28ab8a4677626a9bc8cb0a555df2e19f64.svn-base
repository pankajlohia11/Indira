    <script>
        function dynamicDataTable(e, f, g) {
            var table4 = $(e).DataTable({
                "aoColumnDefs": [
                    { 'bSortable': false, 'aTargets': ["no-sort"] }
                ]
            });
            var colvis = new $.fn.dataTable.ColVis(table4);

            $(colvis.button()).insertAfter(g);
            $(colvis.button()).find('button').addClass('btn btn-greensea btn-xs mb-10').removeClass('ColVis_Button');

            var tt = new $.fn.dataTable.TableTools(table4, {
                sRowSelect: 'single',
                "aButtons": [
                    'copy',
                    'print'
                ],
                "sSwfPath": "../Assets/js/vendor/datatables/extensions/TableTools/swf/copy_csv_xls_pdf.swf"
            });

            $(tt.fnContainer()).insertAfter(f);
            //*initialize responsive datatable
        }
        toastr.options = {
            "closeButton": true,
            "debug": false,
            "positionClass": "toast-top-center",
            "onclick": null,
            "showDuration": "1000",
            "hideDuration": "1000",
            "timeOut": "50000",
            "extendedTimeOut": "1000",
            "showEasing": "swing",
            "hideEasing": "linear",
            "showMethod": "show",
            "hideMethod": "fadeOut"
        };
    </script>

    <script>
        $(window).load(function () {
            $('#form1').parsley().subscribe('parsley:field:validate', function () {
                if ($('#form1').parsley().isValid()) {
                    $('#formSubmit').prop('disabled', false);
                } else {
                    $('#formSubmit').prop('disabled', true);
                }
            });

            $('#formSubmit').on('click', function () {
                $('#form1').submit();
            });

            $('#form2').parsley().subscribe('parsley:field:validate', function () {
                if ($('#form2').parsley().isValid()) {
                    $('#formSubmit1').prop('disabled', false);
                } else {
                    $('#formSubmit1').prop('disabled', true);
                }
            });

            $('#formSubmit1').on('click', function () {
                $('#form2').submit();
            });

        });

        $(document).ready(function () {
            // get current URL path and assign 'active' class
            var pathname = window.location.pathname;
            if (pathname == "/ET_Admin_CustomerModule/ET_Admin_CustomerModule") {
                $('#navigation > li > a[id="a_customermodule"]').parent().addClass('active');
            }
            else {
                $('#navigation > li > ul > li > a[href="..' + pathname + '"]').parent().addClass('active');
                $('#navigation > li > ul > li > a[href="..' + pathname + '"]').parent().parent().parent().addClass('active open dropdown');
            }

        })


        function isNumber(evt) {
            evt = (evt) ? evt : window.event;
            var charCode = (evt.which) ? evt.which : evt.keyCode;
            if (charCode > 31 && (charCode < 48 || charCode > 57)) {
                return false;
            }
            return true;
        }
        function DisableCutCopyPaste(id) {
            $('#' + id).on("cut copy paste", function (e) {
                e.preventDefault();
            });
        }

    </script>
