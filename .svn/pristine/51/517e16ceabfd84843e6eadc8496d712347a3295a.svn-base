                                        <section class="tile time-simple" ng-repeat="item in CustomerMasterList">


                                            <!-- tile body -->
                                            <div class="tile-body">


                                                <!-- row -->
                                                <div class="row">

                                                    <!-- col -->
                                                    <div class="col-md-8">
                                                        <p class="text-default lt">Created:{{item.CREATED_DATE.replace('/Date(','').replace(')/','') | date:'dd-MMM-yyyy HH:mm:ss'}}</p>
                                                        <p class="text-uppercase text-strong mt-40 mb-0 custom-font">Status</p>
                                                        <h3 class="text-uppercase text-success mt-0 mb-20">{{item.activeStatus}}
                                                            
                                                        </h3>
                                                    </div>
                                                    <!-- /col -->
                                                    <!-- col -->
