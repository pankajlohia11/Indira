                                                                <td>{{contact.FIRST_NAME}}</td>
                                                                <td>{{contact.LAST_NAME}}</td>
                                                                <td>{{contact.JOB_TITLE}}</td>
                                                                <td>{{contact.PHONE}}</td>
                                                                <td>{{contact.FAX}}</td>
                                                                <td>{{contact.MOBILE}}</td>
                                                                <td>{{contact.EMAIL}}</td>
                                                                <td>{{contact.REMARKS}}</td>
                                                            </tr>
                                                        
                                                    </tbody>
                                                </table>
                                            </div>

                                        </div>
                                        <!-- /tile body -->

                                    </section>
                                    <!-- /tile -->


                                </div>
                                <!-- /col -->
                            </div>
                            <!-- /row -->



                        </div>

