@model IEnumerable<BusinessEntity.CustomModels.RoleDetails_Role_Document_CM>
@foreach (var item in Model)
{
<tr class="odd gradeX">
    <td style="display:none">@item.FORMS</td>
    <td> @item.FORM_NAME</td>
    <td>
        <label class="checkbox checkbox-custom">
            <input class="chk" type="checkbox" ><i></i>
        </label>
    </td>
    <td>
        <label class="checkbox checkbox-custom">
            <input class="chk" type="checkbox" checked="@item.IS_ADD"><i></i>
        </label>
    </td>
    <td>
        <label class="checkbox checkbox-custom">
            <input class="chk" type="checkbox" checked="@item.IS_EDIT"><i></i>
        </label>
    </td>
    <td>
        <label class="checkbox checkbox-custom">
            <input class="chk" type="checkbox" checked="@item.IS_DELETE"><i></i>
        </label>
    </td>
    <td>
        <label class="checkbox checkbox-custom">
            <input class="chk" type="checkbox" checked="@item.IS_FULLCONTROL"><i></i>
        </label>
    </td>
    <td>
        <label class="checkbox checkbox-custom">
            <input class="chk" type="checkbox" checked="@item.IS_VIEW"><i></i>
        </label>
    </td>
</tr>
}