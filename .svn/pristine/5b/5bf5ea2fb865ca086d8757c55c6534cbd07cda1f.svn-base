    function InsertRecords() {
        var ComID = $("#txtComID").val();
        if (ComID != 0) {
            ComID = $("#RoleID").val();
        }
        else {
            ComID = "0";
        }
        var CompanyName = $("#txtCompanyName").val();
        var DisplayName = $("#txtDisplayName").val();
        var UstId = $("#UstId").val();
        var TaxNo = $("#txtTaxNo").val();
        var Address = $("#txtAddress").val(); 
        var Country = $("#Country").val();
        var State = $(".txtState").val();
        var City = $(".txtCity").val();
        var Zip = $("#txtZip").val();
        var Email = $("#txtEmail").val();
        var Fax = $("#txtFax").val();
        var Phone = $("#txtPhone").val();
        var Mobile = $("#txtMobile").val();
        var Remarks = $("#txtRemarks").val();
        var companytype = $("#companytype").val();
        $.ajax({
                type: "GET",
                url: '@Url.Action("ET_Master_Company_Add", "ET_Admin_Company")',
                contentType: "application/json;charset=utf-8",
                dataType: "json",
            data: {
                    ComID: ComID,
                    CompanyName: CompanyName,
                    DisplayName: DisplayName,
                    UstId: UstId,
                    TaxNo: TaxNo,
                    Address: Address,
                    State: State,
                    Country: Country,
                    City:City,
                    Zip: Zip,
                    Email: Email,
                    Fax: Fax,
                    Phone: Phone,
                    Mobile: Mobile,
                    Remarks: Remarks,
                    companytype: companytype
                },
            success: function (result) {
                    $("#div_View").css("display", "none");
                    $("#div_Edit").css("display", "block");
                    $("#div_company").css("display", "block");
                    $("#div_contacts").css("display", "none");
                    $("#div_BankDetails").css("display", "none");

                alert(result);
                if (result == "Success") {
                        $("#txtCompanyName").val("");
                        $("#txtDisplayName").val("");
                        $("#UstId").val("");
                        $("#txtTaxNo").val("");
                        $("#txtAddress").val("");
                        $(".txtState").val("");
                        $(".txtCity").val("");
                        $("#txtZip").val("");
                        $("#txtEmail").val("");
                        $("#txtFax").val("");
                        $("#txtPhone").val("");
                        $("#txtMobile").val("");
                        $("#txtRemarks").val("");
                        $("#companytype").val("0");
                        $("#txtComID").val("0");
                        alert(result);
                    }
                 
                },
                error: function (response) {

                    alert('eror');
                }
            });
    };
    function createRecords() {
        $("#div_View").css("display", "none");
        $("#div_Edit").css("display", "block");
        $("#div_company").css("display", "block");
        $("#div_contacts").css("display", "none");
        $("#div_BankDetails").css("display", "none");
    };

    function City() {
        var id = ($(".txtState option:selected").val());
         $.ajax({
                type: "GET",
                url: '@Url.Action("Binddropdown_City", "ET_Admin_Company")',
                contentType: "application/json;charset=utf-8",
                dataType: "json",
                data: {id : id },
            success: function (result) {
                var html = '<option value="">--Select--</option>';
                for (i = 0; i < result.length; i++) {
                    html += '<option value="' + result[i].location_id + '">' + result[i].name
                        + '</option>';
                }
                html += '</option>';

                $('.txtCity').html(html);
                },
                error: function (response) {
                    alert('error');
                }
            });
    };

    function State() {
        var id = ($("#Country option:selected").val());
         $.ajax({
                type: "GET",
                url: '@Url.Action("Binddropdown_State", "ET_Admin_Company")',
                contentType: "application/json;charset=utf-8",
                dataType: "json",
                data: {id : id },
             success: function (result) {
                 var html = '<option value="">--Select--</option>';
                 for (i = 0; i < result.length; i++) {
                     html += '<option value="' + result[i].location_id + '">' + result[i].name + '</option>';
                 }
                 html += '</option>';

                 $('.txtState').html(html);
                },
                error: function (response) {
                    alert('error');
                }
            });
    };

