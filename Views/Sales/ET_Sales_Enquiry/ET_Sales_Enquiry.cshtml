                                    <th>Enquiry Date</th>
                                    <th>Enquiry Code</th>
                                    <th>Customer Name</th>
                                    <th ng-if="E_Type==2 || E_Type == 1">Contact Name</th>
                                    <th ng-if="E_Type==2 || E_Type == 1">Sales Person</th>
                                    <th>Total Quantity</th>
                                    <td>Action</td>
                                </tr>

                            </thead>
                            <tbody>
                                <tr ng-repeat="enquiry in enquirylist">
                                    <td>{{enquiry.E_Date.replace('/Date(','').replace(')/','') | date:'yyyy-MM-dd'}}</td>
                                    <td>{{enquiry.E_Code}}</td>
                                    <td>{{enquiry.E_CustomerName}}</td>
                                    <td ng-if="E_Type==2 || E_Type == 1">{{enquiry.E_ContactName}}</td>
                                    <td ng-if="E_Type==2 || E_Type == 1">{{enquiry.E_SalesPersonName}}</td>
                                    <td>{{enquiry.ED_Quantity | customNumberCulture}}</td>
                                    <td>
                                        <a ng-click="editRecords(enquiry.E_ID);" value="{{ enquiry.E_ID }}" style="color:forestgreen;padding:5px;" title="Edit"><i class="fa fa-edit"></i></a>
                                        <a ng-click="ViewRecords(enquiry.E_ID);" value="{{ enquiry.E_ID }}" style="color:darkblue;padding:5px;" title="View"><i class="fa fa-eye"></i></a>
                                        <a ng-click="Restoredeleterecords(enquiry.E_ID,$event,true);" style="color:red;padding:5px;" title="Delete"><i class="fa fa-trash"></i></a>
