                        $('#mailIds').val("");
                        $('#mailCcIds').val("");
                        $('#mailCcIds').val("");
                        $('#sub').val("");
                        $("#summernote").code("");
                         $.ajax({
                type: "GET",
                url: '@Url.Action("GetRfqDetails1", "ET_Sales_Enquiry")',
                data: {
                    id: $("#popIDForMail").val(),
                },
                contentType: "application/json;charset=utf-8",
                dataType: "json",
                success: function (result) {
                    debugger;
                    $("#popupRFQTable").empty();
                    $("#RfqTableId").css("display", "block");
                    var res = JSON.parse(result);
                    for (var i = 0; i < res.length; i++) {
